// Here we have the isprime program again, but this time we'll use our new
// boolean type for the result.
bool ret;
ret := true;
if (arg0 <= 1) then ret := false else skip;
int x;
x := 2;
while (ret && (2*x <= arg0)) do (
  int y;
  y := x;
  while (ret && (y <= arg0)) do (
    if (arg0 <= y) then
      ret := false
    else
      y := y + x
  );
  x := x + 1
)

